<table class="alt">
 <thead>
   <tr>
     <th>Name</th>
     <th>Email</th>
     <th>Admin?</th>
   </tr>
 </thead>

 <tbody>
   <% @users.each do |user| %>
     <tr>
       <td><%= user.name %></td>
       <td><%= user.email %></td>
       <td><%= user.admin %></td>
     </tr>
   <% end %>
 </tbody>
</table>

<%= link_to "Logout", '/logout' %>
